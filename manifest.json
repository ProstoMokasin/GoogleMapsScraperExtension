{
	"manifest_version": 2,
	"name": "Google Maps Scraper Extension",
	"version": "0.1",
	"author": "Mokasin",
	"content_scripts": [
		{
			"matches": [
				"https://www.google.com/maps/search/*"
			],
			"js": [
				"content.js"
			]
		}
	],
	"background": {
		"scripts":["background.js"]	
	},
	"browser_action": {
		"default_icon": "icon.png",
		"default_title": "A popup will appear here."
	}

}